<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.dziekanat">

    <application android:allowBackup="true" android:icon="@mipmap/ic_launcher" android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round" android:supportsRtl="true" android:theme="@style/Theme.Dziekanat">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewUserActivity"
            android:parentActivityName=".MainActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".InsertUserActivity"
            android:parentActivityName=".MainActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".UserInfoAvtivity"
            android:parentActivityName=".ViewUserActivity"
            android:launchMode="singleTop" />
    </application>

</manifest>